# Start/Stop an IPSec connection
ipsec up connectionName
ipsec down connectionName

# Start/Stop/Restart IPSec service
ipsec start
ipsec stop
ipsec restart

# Show detail information abut IPSec connections
ipsec statusall

# Show all loaden CAs
ipsec listcacerts

# Show all loaden certificates
ipsec listcerts

# Reload ipsec.conf settings
ipsec reload

# Update ipsec.conf settings
ipsec update
